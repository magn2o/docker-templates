<?xml version="1.0" encoding="utf-8"?>
<Containers>
<Beta>False</Beta>
<Category>Network:Server</Category>
  <Name>disposble-mailbox</Name>
  <Description>
  A self-hosted disposable mailbox service (aka trash mail) using PHP.
  </Description>
  <Overview>
  Blah
  </Overview>
  <Support>https://github.com/synox/disposable-mailbox</Support>
  <GitHub>https://github.com/synox/disposable-mailbox</GitHub>
  <Repository>magn2o/disposable-mailbox</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
    <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8889</HostPort>
        <ContainerPort>80</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment></Environment>    
  <Data>
    <Volume>
      <HostDir>/mnt/cache/appdata/disposable-mailbox/config.php</HostDir>
      <ContainerDir>/var/config.php</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/cache/appdata/disposable-mailbox/nginx.php</HostDir>
      <ContainerDir>/etc/nginx/conf/default.conf</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI></WebUI>
  <Banner></Banner>
  <Icon></Icon>
</Containers>
